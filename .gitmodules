[submodule "configuration/powerline-fonts"]
	path = configuration/powerline-fonts
	url = https://github.com/Lokaltog/powerline-fonts.git
[submodule "configuration/git-flow-completion"]
	path = configuration/git-flow-completion
	url = https://github.com/bobthecow/git-flow-completion.git
[submodule "configuration/powerline-consolas"]
	path = configuration/powerline-consolas
